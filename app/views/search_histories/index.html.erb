<p style="color: green"><%= notice %></p>

<h1>Search histories</h1>

<div id="search_histories">
  <% @search_histories.each do |search_history| %>
    <%= render search_history %>
    <p>
      <%= link_to "Show this search history", search_history %>
    </p>
  <% end %>
</div>

<%= link_to "New search history", new_search_history_path %>
