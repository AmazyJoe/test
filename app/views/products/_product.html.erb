<div id="<%= dom_id product %>">
  <p>
    <strong>Name:</strong>
    <%= product.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= product.price %>
  </p>

  <p>
    <strong>Rating:</strong>
    <%= product.rating %>
  </p>

  <p>
    <strong>Delivery cost:</strong>
    <%= product.delivery_cost %>
  </p>

  <p>
    <strong>Mode:</strong>
    <%= product.mode %>
  </p>

  <p>
    <strong>Ecommerce website:</strong>
    <%= product.ecommerce_website_id %>
  </p>

</div>
